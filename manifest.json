{
  "manifest_version": 2,
  "name": "Make Chrome Great Again",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
        "js": ["jquery-1.12.0.min.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
    "web_accessible_resources": ["trump_cutouts/trump_cutout01.png"]
}
